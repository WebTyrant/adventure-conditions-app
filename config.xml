<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.adventureconditions" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <hook src="hooks/hookers.js" type="before_prepare" />
    <hook src="hooks/beforedep.js" type="before_deploy" />
    <name>adventureconditions</name>
    <description>
        Brand new cordova project!
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="*" />
    <access origin="*://*.tile.openstreetmap.org" />
    <access origin="https://*.tile.openstreetmap.org" />
    <access origin="*://plume-api.now.sh" />
    <access origin="*://*.cr.usgs.gov" />
    <access origin="*://*.arcgis.com" />
    <access origin="*://maps.alberta.ca" />
    <access origin="*://*.ospo.noaa.gov" />
    <access origin="*://ospo.noaa.gov" />
    <access origin="*://*.noaa.gov" />
    <access origin="*://*.waqi.info" />
    <access origin="*://*.state.or.us" />
    <access origin="*://tripcheck.com" />
    <access origin="*://*.dot.ca.gov" />
    <access origin="*://*.lbl.gov" />
    <access origin="*://cwfis.cfs.nrcan.gc.ca" />
    <access origin="*://gis.fema.gov" />
    <access origin="*://idpgis.ncep.noaa.gov" />
    <access origin="*://geo.weather.gc.ca" />
    <access origin="*://*.traffic.maps.api.here.com" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <engine name="android" spec="^7.1.1" />
    <engine name="browser" spec="^5.0.4" />
</widget>
